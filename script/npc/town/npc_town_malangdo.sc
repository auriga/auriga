-	script	オグノレム#mal	-1,{
	mes "‐イズルードの暖かい日ざしを受けて";
	mes "　気持ちよさそうに寝ている猫がいる‐";
	next;
	if(select("通り過ぎる","話しかける") == 1) {
		mes "‐あなたは、";
		mes "　猫のそばを通り過ぎた‐";
		close;
	}
	mes "[オグノレム]";
	mes "にゃー……";
	mes "ニャン？　ん？";
	mes "猫の手サービスにゃ。";
	mes "猫の楽園、マラン島へ行くにゃ？";
	next;
	if(CATHAND_POINT == 0) {
		mes "[オグノレム]";
		mes "料金は……";
		mes "^FF00001000Zeny^000000かかるにゃ。";
		set '@price,1000;
	} else if(CATHAND_POINT < 60) {
		mes "[オグノレム]";
		mes "猫の手と縁ある人間にゃね。";
		mes "料金は^FF0000999zeny^000000でいいにゃ。";
		set '@price,999;
	} else if(CATHAND_POINT < 80) {
		mes "[オグノレム]";
		mes "いつもお世話になってるにゃ。";
		mes "料金は^FF0000500zeny^000000。";
		mes "半額にゃ。";
		set '@price,500;
	} else {
		mes "[オグノレム]";
		mes "猫の手の英雄は";
		mes "ただでいいにゃ。";
		mes "猫の楽園へ案内するにゃ。";
		set '@price,0;
	}
	next;
	if(select("マラン島へ行く","やめる") == 2) {
		mes "[オグノレム]";
		mes "残念だにゃ。";
		mes "……では、寝るにゃ。";
		close;
	}
	if(Zeny < '@price) {
		mes "[オグノレム]";
		mes "お金、足りてないにゃ。";
		mes "……では、寝るにゃ。";
		close;
	}
	mes "[オグノレム]";
	mes "それじゃあ、マラン島へ出発にゃ〜";
	mes "楽しんでくるにゃ。";
	close2;
	set Zeny,Zeny -'@price;
	warp "malangdo.gat",217,85;
	end;
}

