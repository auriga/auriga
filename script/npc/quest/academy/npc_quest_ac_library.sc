				mes "^0000FF『Alt+Sキー』^000000で表示させたり、";
				mes "消したりすることが出来ます‐";
				next;
				goto L_BASICWINDOW;
			case 6:
				mes "[マップウィンドウについて]";
				mes "‐基本ウィンドウの";
				mes "^0000FFmapボタン^000000をクリックすることで、";
				mes "マップウィンドウを表示させる";
				mes "ことが出来ます‐";
				next;
				mes "[マップウィンドウについて]";
