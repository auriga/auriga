	case 1:
	L_WINDOW:
		mes "[ウィンドウ説明]";
		mes "‐ここでは、ゲーム内に表示される";
		mes "各ウィンドウの説明を行います。";
		mes "調べたい項目を選んでください‐";
		next;
		switch(select("基本ウィンドウ","会話ウィンドウ","ショートカットウィンドウ","交換ウィンドウ","調べるのをやめる")){
		case 1:
			mes "[基本ウィンドウ]";
			mes "‐左上に表示されている、";
			mes "基本情報と書かれたウィンドウが";
			mes "基本ウィンドウです。";
			mes "^0000FF『Alt+Vキー』^000000でウィンドウの大きさを";
			mes "変更することが出来ます‐";
			next;
		L_BASICWINDOW:
			mes "[基本ウィンドウ]";
			mes "‐基本ウィンドウに表示されている、";
			mes "status、option、item、equip、";
			mes "skill、map、comm、friend";
			mes "のボタンをクリックすることで、";
			mes "それぞれの細かい情報を";
			mes "見ることが出来ます‐";
			next;
			switch(select("ステータスウィンドウについて","オプションウィンドウについて","アイテムウィンドウについて","装備ウィンドウについて","スキルウィンドウについて","マップウィンドウについて","チャットウィンドウについて","パーティーウィンドウについて","調べるのをやめる")){
			case 1:
				mes "[ステータスウィンドウについて]";
				mes "‐基本ウィンドウの";
				mes "^0000FFstatusボタン^000000をクリックすることで、";
				mes "ステータスウィンドウを表示させる";
				mes "ことが出来ます‐";
				next;
				mes "[ステータスウィンドウについて]";
				mes "‐ステータスウィンドウでは、";
				mes "自分のキャラクターの";
				mes "ステータス(能力)を確認することが";
				mes "出来ます‐";
				next;
				mes "[ステータスウィンドウについて]";
				mes "‐ステータスウィンドウは";
				mes "^0000FF『Alt+Aキー』^000000で表示させたり、";
				mes "消したりすることが出来ます‐";
				next;
				goto L_BASICWINDOW;
			case 2:
				mes "[オプションウィンドウについて]";
				mes "‐基本ウィンドウの";
				mes "^0000FFoptionボタン^000000をクリックすることで、";
				mes "オプションウィンドウを表示させる";
				mes "ことが出来ます‐";
				next;
				mes "[オプションウィンドウについて]";
				mes "‐オプションウィンドウでは、";
				mes "BGMやEffect、Skinの設定を";
				mes "することが出来ます‐";
				next;
				mes "[オプションウィンドウについて]";
				mes "‐また、オプションウィンドウ";
				mes "下段にある、attack、skill、itemの";
				mes "チェック欄にチェックをいれることで";
				next;
				mes "[オプションウィンドウについて]";
				mes "‐attack(攻撃時)、";
				mes "skill（スキル使用時）、";
				mes "item（アイテム使用時）";
				mes "において、";
				mes "カーソルをターゲッティング";
				mes "しやすくなります‐";
				next;
				mes "[オプションウィンドウについて]";
				mes "‐同様に下段NoCtrlのチェック欄に";
				mes "チェックをいれることで、";
				mes "クリックした敵を攻撃しつづけます‐";
				next;
				mes "[オプションウィンドウについて]";
				mes "‐オプションウィンドウは";
				mes "^0000FF『Alt+Oキー^000000で表示させたり、";
				mes "消したりすることが出来ます‐";
				next;
				goto L_BASICWINDOW;
			case 3:
				mes "[アイテムウィンドウについて]";
				mes "‐基本ウィンドウの";
				mes "^0000FFitemボタン^000000をクリックすることで、";
				mes "アイテムウィンドウを表示させる";
				mes "ことが出来ます‐";
				next;
				mes "[アイテムウィンドウについて]";
				mes "‐アイテムウィンドウでは、";
				mes "キャラクターが所持している";
				mes "アイテムを表示することが出来ます‐";
				next;
				mes "[アイテムウィンドウについて]";
				mes "‐アイテムウィンドウ左側にある";
				mes "item、equip、etcのタブを";
				mes "選択することで、";
				mes "item(消費アイテム)、";
				mes "equip(装備アイテム)、";
				mes "その他(etc)のアイテムを‐";
