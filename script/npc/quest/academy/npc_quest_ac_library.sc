				mes "[アイテムウィンドウについて]";
				mes "‐切り替えることが出来ます。";
				mes "また、各アイテムに";
				mes "カーソルを合わせて、右クリックを";
				mes "することで、アイテムの詳細を";
				mes "別ウィンドウで表示させることが";
				mes "出来ます‐";
				next;
				mes "[アイテムウィンドウについて]";
				mes "‐アイテムウィンドウは";
				mes "^0000FF『Alt+Eキー』^000000で表示させたり、";
				mes "消したりすることが出来ます‐";
				next;
				goto L_BASICWINDOW;
			case 4:
				mes "[装備ウィンドウについて]";
				mes "‐基本ウィンドウの";
				mes "^0000FFequipボタン^000000をクリックすることで、";
				mes "装備ウィンドウを表示させる";
				mes "ことが出来ます‐";
				next;
				mes "[装備ウィンドウについて]";
				mes "‐装備ウィンドウでは、";
				mes "キャラクターが装備している";
				mes "武器、防具を表示することが";
				mes "出来ます‐";
				next;
				mes "[装備ウィンドウについて]";
				mes "‐アイテムウィンドウにある";
				mes "装備品を装備ウィンドウに";
