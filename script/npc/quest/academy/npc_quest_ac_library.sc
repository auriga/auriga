				mes "出来ます‐";
				next;
				mes "[マップウィンドウについて]";
				mes "‐マップウィンドウは";
				mes "^0000FF『Ctrl+Tabキー』^000000を押すことで、";
				mes "表示、半透明表示、非表示を";
				mes "切り替えることが出来ます‐";
				next;
				goto L_BASICWINDOW;
			case 7:
				mes "[チャットウィンドウについて]";
				mes "‐基本ウィンドウの";
				mes "^0000FFcommボタン^000000をクリックすることで、";
				mes "チャットウィンドウを表示させる";
				mes "ことが出来ます‐";
				next;
				mes "[チャットウィンドウについて]";
				mes "‐チャットウィンドウでは、";
				mes "チャットルームを作成することが";
				mes "出来ます‐";
				next;
				mes "[チャットウィンドウについて]";
				mes "‐チャットウィンドウ上段にある";
