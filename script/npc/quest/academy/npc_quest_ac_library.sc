				mes "ダブルクリックすることで、";
				mes "装備を変更することが可能です‐";
				next;
				mes "[装備ウィンドウについて]";
				mes "‐装備ウィンドウは";
				mes "^0000FF『Alt+Qキー』^000000で表示させたり、";
				mes "消したりすることが出来ます‐";
				next;
				goto L_BASICWINDOW;
			case 5:
				mes "[スキルウィンドウについて]";
				mes "‐基本ウィンドウの";
				mes "^0000FFskillボタン^000000をクリックすることで、";
				mes "スキルウィンドウを表示させる";
