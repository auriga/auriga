		mes "[キーボード操作]";
		mes "‐^0000FF『Alt+Dキー』^000000でスプライトや、";
		mes "テクスチャ解像度の";
		mes "調整を行う事ができます‐";
		next;
		mes "[キーボード操作]";
		mes "‐^0000FF『Alt+Homeキー』^000000で、";
		mes "カーソルの下に出る緑色の枠を";
		mes "そこに固定することが出来ます‐";
		next;
		mes "[キーボード操作]";
		mes "‐^0000FF『Alt+Tabキー』^000000で、";
		mes "ラグナロクオンライン以外の";
		mes "ウィンドウへの切り替えを";
		mes "行うことが出来ます‐";
		next;
		mes "[キーボード操作]";
		mes "‐^0000FF『Alt+Endキー』^000000で";
		mes "プレイヤーの下に表示される";
		mes "HP（緑色）ゲージ、SP（青色）ゲージの";
		mes "表示、非表示を";
		mes "切り替えることが出来ます‐";
		next;
		mes "[キーボード操作]";
		mes "‐^0000FF『Escキー』^000000で";
		mes "ログアウト等を行う";
		mes "メニューウィンドウの表示、";
		mes "非表示を切り替えることが出来ます‐";
		next;
		mes "[キーボード操作]";
		mes "‐^0000FF『F1〜F9キー』^000000で";
		mes "それぞれに設定したスキルや、";
		mes "アイテムを使用することが出来ます‐";
		next;
		mes "[キーボード操作]";
		mes "‐^0000FF『F10キー』^000000で、";
		mes "会話ウィンドウの大きさの変更や、";
		mes "表示、非表示の切り替えを";
		mes "行うことが出来ます‐";
		next;
		mes "[キーボード操作]";
		mes "‐^0000FF『F11キー』^000000でほぼ全ての";
		mes "ウィンドウを閉じることが出来ます‐";
		next;
		mes "[キーボード操作]";
		mes "‐^0000FF『F12キー』^000000で";
		mes "ショートカットウィンドウの";
		mes "切り替えを行うことが出来ます‐";
		next;
		mes "[キーボード操作]";
		mes "‐^0000FF『Insertキー』^000000で";
		mes "座ったり、立ったりすることが";
		mes "出来ます‐";
		mes "※基本スキルレベル3以上が必要";
		next;
		mes "[キーボード操作]";
		mes "‐^0000FF『PrintScreenキー』^000000で";
		mes "ゲーム画面をJPEGで保存することが";
		mes "出来ます‐";
		next;
		mes "[キーボード操作]";
		mes "‐^0000FF『Ctrl+PrintScreenキー』^000000で";
		mes "ゲーム画面をBMPで保存することが";
		mes "出来ます‐";
		next;
		mes "[キーボード操作]";
		mes "‐^0000FF『ScrollLockキー』^000000で";
		mes "ゲーム画面をJPEGで保存することが";
		mes "出来ます‐";
		close;
	case 4:
		mes "[スキルの使用]";
		mes "‐スキルウィンドウにあるスキルを";
		mes "ダブルクリックし、";
		mes "対象となる地面、敵、キャラクターに";
		mes "カーソルを合わせた後、";
		mes "再びクリックすることで、";
		mes "スキルを使用することが出来ます‐";
		next;
		mes "[スキルの使用]";
		mes "‐また、あらかじめ";
		mes "ショートカットに登録しておくことで、";
		mes "簡単にスキルを使用することが";
		mes "出来ます‐";
		next;
		mes "[スキルの使用]";
		mes "‐ただし、パッシブスキルと";
		mes "表示されているスキルは";
		mes "常時キャラクターに影響を及ぼす";
		mes "スキルであり、";
		mes "対象を選ぶことが出来ません‐";
		next;
		mes "[スキルの使用]";
		mes "‐レベルを変更出来るスキルを";
		mes "使用する場合は原則的に、";
		mes "スキルウィンドウで設定している";
		mes "レベルのスキルが使用されますが、";
		close;
	case 5:
		mes "[アイテムの使用]";
		mes "‐アイテムウィンドウにあるアイテムを";
		mes "ダブルクリックすることで";
		mes "アイテムを使用することが出来ます‐";
		next;
		mes "[アイテムの使用]";
		mes "‐また、あらかじめ";
		mes "ショートカットに登録しておくことで、";
		mes "簡単にアイテムを使用することが";
		mes "出来ます‐";
		close;
	case 6:
		mes "[ショートカット登録]";
		mes "‐アイテムやアクティブスキルを";
		mes "ショートカットウィンドウにある";
		mes "空欄にドラッグ&ドロップすることで、";
		mes "^0000FF『F1〜F9キー』^000000に登録しておくことが";
		mes "出来ます‐";
		next;
		mes "[ショートカット登録]";
		mes "‐ショートカットウィンドウの";
		mes "9つの空欄はそれぞれ左から、";
		mes "^0000FF『F1』、『F2』、『F3』、『F4』、";
		mes "『F5』、『F6』、『F7』、『F8』、";
		mes "『F9』キー^000000に対応しています‐";
		close;
	case 7:
	L_COMMAND:
		mes "[コマンドリスト]";
		mes "‐会話ウィンドウに、様々なコマンドを";
		mes "入力することで、システムの変更、";
		mes "情報の表示、その他チャットや、";
		mes "パーティー作成、等を行うことが";
		mes "出来ます‐";
		next;
		switch(select("パーティー、スキル関係のコマンド","利便性の高いコマンド","その他のコマンド")){
		case 1:
			mes "[パーティー、スキル関係のコマンド]";
			mes "‐^0000FF/organize [パーティー名]^000000";
			mes "と入力することで、";
			mes "自分をリーダーとして、";
			mes "パーティーを作成することが出来ます‐";
			mes "※/organizeの後は要半角スペース";
			next;
			mes "[パーティー、スキル関係のコマンド]";
			mes "‐^0000FF/leave^000000";
			mes "と入力することで、";
			mes "パーティーから";
			mes "脱退することが出来ます‐";
			next;
			mes "[パーティー、スキル関係のコマンド]";
			mes "‐^0000FF/memo^000000";
			mes "と入力することで、";
			mes "ワープポータルスキルの";
			mes "ワープ先を記憶しておくことが";
			mes "出来ます‐";
			next;
			goto L_COMMAND;
		case 2:
			mes "[利便性の高いコマンド]";
			mes "‐^0000FF/savechat^000000";
			mes "と入力することで、";
			mes "会話のログを保存しておくことが";
			mes "出来ます‐";
			next;
			mes "[利便性の高いコマンド]";
			mes "‐^0000FF/bm^000000または^0000FF/battlemode^000000";
			mes "と入力することで、";
			mes "ショートカットウィンドウ[2]を、";
			mes "^0000FF『Q〜Oキー』^000000に、";
			mes "ショートカットウィンドウ[3]を";
			mes "^0000FF『A〜Lキー』^000000に、";
			next;
			mes "[利便性の高いコマンド]";
			mes "ショートカットウィンドウ[1]を、";
			mes "^0000FF『F1〜F9キー』^000000と^0000FF『Z〜＞キー』^000000に";
			mes "割り当てることが出来ます。";
			mes "（それぞれ一番左端のキーが";
			mes "^0000FF『F1キー』^000000、一番右端のキーが";
			mes "^0000FF『F9キー』^000000となります）‐";
			next;
			mes "[利便性の高いコマンド]";
			mes "‐このモードを解除し、";
			mes "通常会話する場合は、";
			mes "キーボードを半角入力モードにし、";
			mes "チャット入力欄で、スペースキー、";
			mes "もしくはエンターキーを一度押したあと";
			mes "再度コマンドを入力してください‐";
			next;
			goto L_COMMAND;
		case 3:
			mes "[その他のコマンド]";
			mes "‐^0000FF/h^000000または、^0000FF/help^000000";
			mes "と入力することで、コマンドのリストを";
			mes "開くことが出来、その他のコマンドを";
			mes "確認することが出来ます‐";
			next;
			mes "[その他のコマンド]";
			mes "‐また、^0000FF『Alt+Yキー』^000000で";
			mes "コマンドリストが表示されている";
			mes "ウィンドウを表示することが";
			mes "出来ます‐";
			next;
			mes "[その他のコマンド]";
			mes "‐このウィンドウでは、";
			mes "コマンドの確認だけで無く、";
			mes "ON/OFFを切り替えることが";
			mes "出来ます‐";
			next;
			goto L_COMMAND;
		}
	default:
	}
L_CLOSE:
	mes "["+ strcharinfo(0) +"]";
	mes "（もう調べることはないかな。）";
	close;
}
ac_library.gat,103,12,6	script	戦闘関連	837,{
L_TOPMENU:
	mes "[" + strcharinfo(0) + "]";
	mes "（戦闘関連についての";
	mes "本が並んでいる。";
	mes "何について調べよう……）";
	next;
	switch(select("戦闘の方法","パーティーについて","調べるのをやめる")){
	case 1:
	L_BATTLE:
		mes "[戦闘の方法]";
		mes "‐ここでは、ゲーム内での様々な";
		mes "戦闘方法について説明を行います。";
		mes "調べたい項目を選んでください‐";
		next;
		switch(select("武器による直接攻撃","武器による遠距離攻撃","スキルによる攻撃","回復方法","調べるのをやめる")){
		case 1:
			mes "[武器による直接攻撃]";
			mes "‐剣や鈍器等の近接攻撃用の武器を";
			mes "装備している時に、";
			mes "対象にカーソルを合わせると、";
			mes "カーソルが剣の形に変化します‐";
			next;
			mes "[武器による直接攻撃]";
			mes "その状態で左クリックすることで、";
			mes "対象に近づいて、";
			mes "攻撃することが出来ます。";
			mes "ソードマン系、シーフ系、";
			mes "マーチャント系等、近距離攻撃職の";
			mes "主な攻撃手段となります‐";
			next;
			mes "[武器による直接攻撃]";
			mes "‐左クリックを押しっぱなしに";
			mes "することで、継続して攻撃を行えます‐";
			next;
			mes "[武器による直接攻撃]";
			mes "‐また、あらかじめ会話ウィンドウに";
			mes "^0000FF/noctrl^000000と入力しておくか、";
			mes "オプションウィンドウのnoctrlの欄に";
			mes "チェックを入れておくことで、";
			mes "一度のクリックで半永久的に";
			mes "攻撃を行うことが出来ます‐";
			next;
			goto L_BATTLE;
		case 2:
			mes "[武器による遠距離攻撃]";
			mes "‐弓や銃等の遠距離攻撃用の武器を";
			mes "装備しているときに、";
			mes "対象にカーソルを合わせると、";
			mes "カーソルが剣の形に変化します‐";
			next;
			mes "[武器による遠距離攻撃]";
			mes "‐その状態で左クリックすることで、";
			mes "攻撃可能な範囲に移動して、";
			mes "攻撃することが出来ます。";
			mes "アーチャー系、ガンスリンガー系等、";
			mes "遠距離攻撃職の";
			mes "主な攻撃手段となります‐";
			next;
			mes "[武器による遠距離攻撃]";
			mes "但し、弓で攻撃する時は『矢』が、";
			mes "銃で攻撃する時は『弾』が";
			mes "それぞれ必要になります。";
			mes "『矢』と『弾』は攻撃を行うたびに";
			mes "消費していきますので";
			mes "注意してください‐";
			next;
			mes "[武器による遠距離攻撃]";
			mes "‐左クリックを押しっぱなしに";
			mes "することで、継続して攻撃を行えます‐";
			next;
			mes "[武器による遠距離攻撃]";
			mes "‐また、あらかじめ会話ウィンドウに";
			mes "^0000FF/noctrl^000000と入力しておくか、";
			mes "オプションウィンドウの/noctrlの欄に";
			mes "チェックを入れておくことで、";
			mes "一度のクリックで半永久的に";
			mes "攻撃を行うことが出来ます‐";
			next;
			goto L_BATTLE;
		case 3:
			mes "[スキルによる攻撃]";
			mes "‐SPを消費することで、";
			mes "スキルによる攻撃を行うことが";
			mes "出来ます。";
			mes "職業により、覚えられるスキルは";
			mes "異なり、属性攻撃や、範囲攻撃‐";
			next;
			mes "[スキルによる攻撃]";
			mes "‐または自分やパーティーの能力を";
			mes "高める等効果は様々です‐";
			next;
			mes "[スキルによる攻撃]";
			mes "‐使用したいスキルを";
			mes "ダブルクリックすると、";
			mes "カーソルが輪の形になります。";
			mes "その状態で対象にカーソルを合わせ、";
			mes "左クリックすることで対象に";
			mes "スキルを使用することが出来ます‐";
			next;
			goto L_BATTLE;
		case 4:
			mes "[回復方法]";
			mes "‐モンスターとの戦闘で受けた";
			mes "ダメージは様々な手段で";
			mes "回復することが出来ます‐";
			next;
			mes "[回復方法]";
			mes "‐一つ目の方法は^0000FF座る^000000です。";
			mes "キャラクターの移動させないと";
			mes "HPはじょじょに回復しますが、";
			mes "^0000FFInsertキー^000000を押して、キャラクターを";
			mes "座らせると、自然回復が";
			mes "少し早くなります‐";
			next;
			mes "[回復方法]";
			mes "‐ニつ目の方法は^0000FFアイテム^000000です。";
			mes "ハーブや果物、野菜、飲み物、";
			mes "ポーション類等の回復アイテムを";
			mes "使用することで";
			mes "HPやSPを回復させることが出来ます‐";
			next;
			mes "[回復方法]";
			mes "‐三つ目の方法は^0000FFスキル^000000です。";
			mes "アコライト等のスキルでHPや";
			mes "SPを回復させることが出来ます。";
			mes "スキルには直接回復させる";
			mes "方法だけで無く、更に自然回復を";
			mes "付加するもの等もあります‐";
			next;
			mes "[回復方法]";
			mes "‐四つ目の方法は^0000FF宿屋^000000です。";
			mes "街にある宿屋を利用すると、";
			mes "Zenyを消費して、HPとSPを";
			mes "全快させることが出来ます‐";
			next;
			mes "[回復方法]";
			mes "‐その他に、";
			mes "キャラクターのレベルが";
			mes "アップした時にも";
			mes "HPとSPが回復します‐";
			next;
			goto L_BATTLE;
		default:
			goto L_TOPMENU;
		}
	case 2:
		mes "[パーティーについて]";
		mes "‐複数のプレイヤーでパーティーを";
		mes "組めば、一人では倒せない";
		mes "モンスターを倒したり、";
		mes "行ったことの無いダンジョンに";
		mes "挑戦することが出来るようになります‐";
		next;
		mes "[パーティーについて]";
		mes "‐また、パーティーメンバーの";
		mes "最低レベルと最高レベルの差が";
		mes "10以内の場合、";
		mes "与えたダメージに関係なく、";
		mes "経験値を公平に分配することが";
		mes "出来ます‐";
		next;
		mes "[パーティーについて]";
		mes "‐パーティーを作成する場合は";
		mes "会話ウィンドウに";
		mes "^0000FF/organize （パーティー名）^000000";
		mes "と入力します‐";
		mes "※/organizeの後ろは要半角スペース";
		next;
		mes "[パーティーについて]";
		mes "‐表示されたウィンドウで、";
		mes "アイテム分配の設定をし、";
		mes "OKボタンをクリックすると、";
		mes "パーティーが作成されます‐";
		next;
		mes "[パーティーについて]";
		mes "‐作成されたパーティーの情報は";
		mes "基本ウィンドウのfriendボタンを";
		mes "クリックした時に表示される、";
		mes "パーティーウィンドウで確認することが";
		mes "出来ます‐";
		next;
		mes "[パーティーについて]";
		mes "‐パーティーウィンドウでは、";
		mes "パーティーメンバーのリストと、";
		mes "ログイン、ログアウト等の情報を";
		mes "確認することが出来ます‐";
		next;
		mes "[パーティーについて]";
		mes "‐また、リーダーであれば、";
		mes "経験値の公平分配等の";
		mes "パーティー設定の変更を行うことも";
		mes "出来ます。";
		next;
		goto L_TOPMENU;
	}
L_CLOSE:
	mes "["+ strcharinfo(0) +"]";
	mes "（もう調べることはないかな。）";
	close;
}
