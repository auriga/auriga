				mes "入室可能人数を指定します‐";
				next;
				mes "[チャットウィンドウについて]";
				mes "‐その後、最下段のOKボタンを";
				mes "クリックすることで、";
				mes "チャットルームを作成することが";
				mes "出来ます‐";
				next;
				mes "[チャットウィンドウについて]";
				mes "‐また、下段Publicのチェックを";
				mes "非公開に入れ、";
				mes "Signにパスワードを入力することで、";
				mes "必要に応じてチャットルームの";
				mes "入室に必要なパスワードを";
				mes "設定することが出来ます‐";
				next;
				mes "[チャットウィンドウについて]";
				mes "‐チャットウィンドウは";
				mes "^0000FF『Art+Cキー』^000000で表示させたり、";
				mes "消したりすることが出来ます‐";
				next;
				goto L_BASICWINDOW;
			case 8:
				mes "[パーティーウィンドウについて]";
				mes "‐基本ウィンドウの";
				mes "^0000FFfriendボタン^000000を";
				mes "クリックすることで、";
				mes "パーティーウィンドウを表示させる";
				mes "ことが出来ます‐";
				next;
				mes "[パーティーウィンドウについて]";
				mes "‐パーティーウィンドウでは、";
				mes "パーティーメンバーのリストと、";
				mes "ログイン、ログアウトといった";
				mes "情報を確認することが出来ます‐";
				next;
				mes "[パーティーウィンドウについて]";
				mes "‐また、パーティー設定の変更、";
				mes "確認や、友達リストの編集も";
				mes "行うことが出来ます‐";
				next;
				mes "[パーティーウィンドウについて]";
				mes "‐友達リストに登録する場合は、";
				mes "登録したいキャラクターにカーソルを";
				mes "合わせ、右クリックした時に出る";
				mes "友達に登録する";
				mes "を選択してください‐";
				next;
				mes "[パーティーウィンドウについて]";
				mes "‐パーティーウィンドウは";
				mes "^0000FF『Alt+Zキー』^000000で表示させたり、";
				mes "消したりすることが出来ます‐";
				next;
				goto L_BASICWINDOW;
			default:
				goto L_WINDOW;
			}
		case 2:
			mes "[会話ウィンドウについて]";
			mes "‐ゲームにログインした時に";
			mes "下段に表示されるウィンドウが";
			mes "会話ウィンドウです‐";
			next;
			mes "[会話ウィンドウについて]";
			mes "‐会話ウィンドウ左上にある";
			mes "会話、バトルタブを";
			mes "切り替えることによって、";
			mes "スキル使用等の";
			mes "戦闘に関わるメッセージの";
			mes "表示、非表示の切り替えが出来ます‐";
			next;
			mes "[会話ウィンドウについて]";
			mes "‐会話、バトルタブの横にある、";
			mes ">>ボタンを押すことで、";
			mes "会話タブ状態の会話ウィンドウと";
			mes "バトルタブ状態の会話ウィンドウの";
			mes "両方を表示させることが出来ます‐";
			next;
			mes "[会話ウィンドウについて]";
			mes "‐両方のウィンドウが表示されている";
			mes "状態で、<<ボタンを押すことで、";
			mes "二つのウィンドウを元の";
			mes "ウィンドウにまとめることが出来ます‐";
			next;
			mes "[会話ウィンドウについて]";
			mes "‐>>ボタンの右にある+ボタンを";
			mes "押すことで、バトルウィンドウに";
			mes "表示されるメッセージの設定が";
			mes "出来ます‐";
			next;
			mes "[会話ウィンドウについて]";
			mes "‐会話タブ、バトルタブの";
			mes "両会話ウィンドウでは、";
			mes "下段右側の空欄にメッセージを入力し";
			mes "^0000FF『Enterキー』^000000を押すことで、";
			mes "発言することが出来ます‐";
			next;
			mes "[会話ウィンドウについて]";
			mes "‐その際、下段左側の空欄に";
			mes "キャラクターの名前を入力すると、";
			mes "そのキャラクターにのみ聞こえる";
			mes "発言を行うことが出来ます‐";
			next;
			mes "[会話ウィンドウについて]";
			mes "‐その他にも、下段右側にある";
			mes "二つのボタンのうち、";
			mes "左側のボタンを押すことで、";
			mes "全体会話、パーティー会話、";
			mes "ギルド会話を切り替えることが";
			mes "出来ます‐";
			next;
			mes "[会話ウィンドウについて]";
			mes "‐下段右側にある二つのボタンのうち、";
			mes "右側のボタンを押すか、";
			mes "^0000FF『fF10キー』^000000で、";
			mes "ウィンドウサイズの変更を";
			mes "することが出来ます‐";
			next;
			goto L_WINDOW;
		case 3:
			mes "[ショートカットウィンドウについて]";
			mes "‐ショートカットウィンドウは、";
			mes "^0000FF『F12キー』^000000で、";
			mes "切り替えることが出来ます‐";
			next;
			mes "[ショートカットウィンドウについて]";
			mes "‐ショートカットウィンドウでは、";
			mes "アイテムやアクティブスキルを";
			mes "ショートカットウィンドウにある";
			mes "空欄にドラッグすることで、";
			mes "^0000FF『F1』〜『F9』キー^000000に";
			mes "登録しておくことが出来ます‐";
			next;
			mes "[ショートカットウィンドウについて]";
			mes "‐ショートカットウィンドウの";
			mes "9つの空欄はそれぞれ左から、";
			mes "^0000FF『F1』、『F2』、『F3』、『F4』、";
			mes "『F5』、『F6』、『F7』、『F8』、";
			mes "『F9』キー^000000に対応しています‐";
			next;
			goto L_WINDOW;
		case 4:
			mes "[交換ウィンドウについて]";
			mes "‐キャラクターにカーソルを合わせ、";
			mes "右クリックした時に表示される";
			mes "ウィンドウから取引要請を";
			mes "選択することで";
			mes "キャラクターに取引要請を";
			mes "行うことが出来ます‐";
			next;
			mes "[交換ウィンドウについて]";
			mes "‐取引要請のウィンドウに対して、";
			mes "相手がOKボタンをクリックすると、";
			mes "交換ウィンドウが表示されます";
